<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage async="false" continueOnError="false" enabled="true" name="Set-Subscriber-URL">
    <DisplayName>Set Subscriber URL</DisplayName>
    <Properties/>
    <AssignVariable>
        <Name>target.url</Name>
        <Value>/ssridhar/sandbox/topics/{topicname}/subscribers</Value>
    </AssignVariable>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
    <AssignTo createNew="false" transport="http" type="request"/>
</AssignMessage>